namespace DevToolsX.TempConsole;
generator TestDocWiki : TestWikiGenerator;

using DevToolsX.Testing.Selenium;

template OpenPage(string url, string search)
[Element input]
[Element searchButton]
Open page '[url]' in the browser: [void Browser.GoToUrl(url)]
^
[Screenshot.TakeScreenshot()]
^
<br/>
^
The page title should be [Browser.TitleShouldBe("Google")].
^
[input = Page.FindElement("name:q")]
Type '[search]' into the search box: [void input.TypeText(search)]
^
[void input.Mark()]
[void Wait.For(TimeSpan.FromSeconds(3))]
[Screenshot.TakeScreenshot()]
[void input.Unmark()]
^
[searchButton = Page.FindElements("xpath://input[@class='lsb']")[0]]
Press the <b>[searchButton.Value]</b> button:
^
[void searchButton.Mark()]
[Screenshot.TakeScreenshot()]
[void searchButton.Unmark()]
^
[void searchButton.Click()]
[void Wait.For(TimeSpan.FromSeconds(3))]
^
[Screenshot.TakeScreenshot()]
^
end template

